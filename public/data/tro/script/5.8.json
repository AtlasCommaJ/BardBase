[{"player": "SCENE", "playerline": "SCENE VIII"}, {"player": "DIRECTION", "playerline": "[Enter Thersites; then Menelaus fighting Paris.]"}, {"player": "THERSITES", "playerline": "The cuckold and the cuckold-maker are at"}, {"player": "THERSITES", "playerline": "it. Now, bull! Now, dog! Loo, Paris, loo! Now, my"}, {"player": "THERSITES", "playerline": "double-horned Spartan! Loo, Paris, loo! The bull"}, {"player": "THERSITES", "playerline": "has the game. Ware horns, ho!"}, {"player": "DIRECTION", "playerline": "[Paris and Menelaus exit, fighting.]"}, {"player": "DIRECTION", "playerline": "[Enter Bastard.]"}, {"player": "BASTARD", "playerline": "Turn, slave, and fight."}, {"player": "THERSITES", "playerline": "What art thou?"}, {"player": "BASTARD", "playerline": "A bastard son of Priam's."}, {"player": "THERSITES", "playerline": "I am a bastard too. I love bastards. I am"}, {"player": "THERSITES", "playerline": "bastard begot, bastard instructed, bastard in mind,"}, {"player": "THERSITES", "playerline": "bastard in valor, in everything illegitimate. One"}, {"player": "THERSITES", "playerline": "bear will not bite another, and wherefore should"}, {"player": "THERSITES", "playerline": "one bastard? Take heed: the quarrel's most ominous"}, {"player": "THERSITES", "playerline": "to us. If the son of a whore fight for a whore,"}, {"player": "THERSITES", "playerline": "he tempts judgment. Farewell, bastard."}, {"player": "DIRECTION", "playerline": "[He exits.]"}, {"player": "BASTARD", "playerline": "The devil take thee, coward!"}, {"player": "DIRECTION", "playerline": "[He exits.]"}]