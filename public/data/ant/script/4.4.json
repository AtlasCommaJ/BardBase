[{"player": "SCENE", "playerline": "SCENE IV"}, {"player": "DIRECTION", "playerline": "[Enter Antony and Cleopatra, with"}, {"player": "DIRECTION", "playerline": "Charmian, and others.]"}, {"player": "ANTONY", "playerline": "Eros! Mine armor, Eros!"}, {"player": "CLEOPATRA", "playerline": "Sleep a little."}, {"player": "ANTONY", "playerline": "No, my chuck.--Eros, come, mine armor, Eros."}, {"player": "DIRECTION", "playerline": "[Enter Eros, carrying armor.]"}, {"player": "ANTONY", "playerline": "Come, good fellow, put thine iron on."}, {"player": "ANTONY", "playerline": "If fortune be not ours today, it is"}, {"player": "ANTONY", "playerline": "Because we brave her. Come."}, {"player": "CLEOPATRA", "playerline": "Nay, I'll help too."}, {"player": "CLEOPATRA", "playerline": "What's this for?"}, {"player": "ANTONY", "playerline": "Ah, let be, let be! Thou art"}, {"player": "ANTONY", "playerline": "The armorer of my heart. False, false. This, this!"}, {"player": "CLEOPATRA", "playerline": "Sooth, la, I'll help. Thus it must be."}, {"player": "ANTONY", "playerline": "Well, well,"}, {"player": "ANTONY", "playerline": "We shall thrive now.--Seest thou, my good fellow?"}, {"player": "ANTONY", "playerline": "Go, put on thy defenses."}, {"player": "EROS", "playerline": "Briefly, sir."}, {"player": "CLEOPATRA", "playerline": "Is not this buckled well?"}, {"player": "ANTONY", "playerline": "Rarely, rarely."}, {"player": "ANTONY", "playerline": "He that unbuckles this, till we do please"}, {"player": "ANTONY", "playerline": "To daff 't for our repose, shall hear a storm.--"}, {"player": "ANTONY", "playerline": "Thou fumblest, Eros, and my queen's a squire"}, {"player": "ANTONY", "playerline": "More tight at this than thou. Dispatch.--O love,"}, {"player": "ANTONY", "playerline": "That thou couldst see my wars today, and knew'st"}, {"player": "ANTONY", "playerline": "The royal occupation, thou shouldst see"}, {"player": "ANTONY", "playerline": "A workman in 't."}, {"player": "DIRECTION", "playerline": "[Enter an armed Soldier.]"}, {"player": "ANTONY", "playerline": "Good morrow to thee. Welcome."}, {"player": "ANTONY", "playerline": "Thou look'st like him that knows a warlike charge."}, {"player": "ANTONY", "playerline": "To business that we love we rise betime"}, {"player": "ANTONY", "playerline": "And go to 't with delight."}, {"player": "SOLDIER", "playerline": "A thousand, sir,"}, {"player": "SOLDIER", "playerline": "Early though 't be, have on their riveted trim"}, {"player": "SOLDIER", "playerline": "And at the port expect you."}, {"player": "DIRECTION", "playerline": "[Shout. Trumpets flourish.]"}, {"player": "DIRECTION", "playerline": "[Enter Captains and Soldiers.]"}, {"player": "CAPTAIN", "playerline": "The morn is fair. Good morrow, general."}, {"player": "ALL", "playerline": "Good morrow, general."}, {"player": "ANTONY", "playerline": "'Tis well blown, lads."}, {"player": "ANTONY", "playerline": "This morning, like the spirit of a youth"}, {"player": "ANTONY", "playerline": "That means to be of note, begins betimes."}, {"player": "ANTONY", "playerline": "So, so.--Come, give me that. This way.--Well said.--"}, {"player": "ANTONY", "playerline": "Fare thee well, dame."}, {"player": "DIRECTION", "playerline": "[He kisses her.]"}, {"player": "ANTONY", "playerline": "Whate'er becomes of me,"}, {"player": "ANTONY", "playerline": "This is a soldier's kiss. Rebukable"}, {"player": "ANTONY", "playerline": "And worthy shameful check it were to stand"}, {"player": "ANTONY", "playerline": "On more mechanic compliment. I'll leave thee"}, {"player": "ANTONY", "playerline": "Now like a man of steel.--You that will fight,"}, {"player": "ANTONY", "playerline": "Follow me close. I'll bring you to 't.--Adieu."}, {"player": "DIRECTION", "playerline": "[Antony, Eros, Captains, and Soldiers exit.]"}, {"player": "CHARMIAN", "playerline": "Please you retire to your chamber?"}, {"player": "CLEOPATRA", "playerline": "Lead me."}, {"player": "CLEOPATRA", "playerline": "He goes forth gallantly. That he and Caesar might"}, {"player": "CLEOPATRA", "playerline": "Determine this great war in single fight,"}, {"player": "CLEOPATRA", "playerline": "Then Antony--but now--. Well, on."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]