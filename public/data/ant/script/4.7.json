[{"player": "SCENE", "playerline": "SCENE VII"}, {"player": "DIRECTION", "playerline": "[Alarum, Drums and Trumpets. Enter Agrippa,"}, {"player": "DIRECTION", "playerline": "with other of Caesar's soldiers.]"}, {"player": "AGRIPPA", "playerline": "Retire! We have engaged ourselves too far."}, {"player": "AGRIPPA", "playerline": "Caesar himself has work, and our oppression"}, {"player": "AGRIPPA", "playerline": "Exceeds what we expected."}, {"player": "DIRECTION", "playerline": "[They exit.]"}, {"player": "DIRECTION", "playerline": "[Alarums. Enter Antony, and Scarus wounded.]"}, {"player": "SCARUS", "playerline": "O my brave emperor, this is fought indeed!"}, {"player": "SCARUS", "playerline": "Had we done so at first, we had droven them home"}, {"player": "SCARUS", "playerline": "With clouts about their heads."}, {"player": "ANTONY", "playerline": "Thou bleed'st apace."}, {"player": "SCARUS", "playerline": "I had a wound here that was like a T,"}, {"player": "SCARUS", "playerline": "But now 'tis made an H."}, {"player": "DIRECTION", "playerline": "[Sound of retreat far off.]"}, {"player": "ANTONY", "playerline": "They do retire."}, {"player": "SCARUS", "playerline": "We'll beat 'em into bench-holes. I have yet"}, {"player": "SCARUS", "playerline": "Room for six scotches more."}, {"player": "DIRECTION", "playerline": "[Enter Eros.]"}, {"player": "EROS", "playerline": "They are beaten, sir, and our advantage serves"}, {"player": "EROS", "playerline": "For a fair victory."}, {"player": "SCARUS", "playerline": "Let us score their backs"}, {"player": "SCARUS", "playerline": "And snatch 'em up as we take hares, behind."}, {"player": "SCARUS", "playerline": "'Tis sport to maul a runner."}, {"player": "ANTONY", "playerline": "I will reward thee"}, {"player": "ANTONY", "playerline": "Once for thy sprightly comfort and tenfold"}, {"player": "ANTONY", "playerline": "For thy good valor. Come thee on."}, {"player": "SCARUS", "playerline": "I'll halt after."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]