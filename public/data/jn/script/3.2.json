[{"player": "SCENE", "playerline": "SCENE II"}, {"player": "DIRECTION", "playerline": "[Alarums, excursions."}, {"player": "DIRECTION", "playerline": "Enter Bastard with Austria's head.]"}, {"player": "BASTARD", "playerline": "Now, by my life, this day grows wondrous hot."}, {"player": "BASTARD", "playerline": "Some airy devil hovers in the sky"}, {"player": "BASTARD", "playerline": "And pours down mischief. Austria's head lie there,"}, {"player": "BASTARD", "playerline": "While Philip breathes."}, {"player": "DIRECTION", "playerline": "[Enter King John, Arthur, Hubert.]"}, {"player": "KING JOHN", "playerline": "Hubert, keep this boy.--Philip, make up."}, {"player": "KING JOHN", "playerline": "My mother is assailed in our tent"}, {"player": "KING JOHN", "playerline": "And ta'en, I fear."}, {"player": "BASTARD", "playerline": "My lord, I rescued her."}, {"player": "BASTARD", "playerline": "Her Highness is in safety, fear you not."}, {"player": "BASTARD", "playerline": "But on, my liege, for very little pains"}, {"player": "BASTARD", "playerline": "Will bring this labor to an happy end."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]