[{"player": "SCENE", "playerline": "SCENE II"}, {"player": "DIRECTION", "playerline": "[Enter Jaques and Lords, like foresters.]"}, {"player": "JAQUES", "playerline": "Which is he that killed the deer?"}, {"player": "FIRST LORD", "playerline": "Sir, it was I."}, {"player": "JAQUES", "playerline": "Let's present him to the"}, {"player": "JAQUES", "playerline": "Duke like a Roman conqueror. And it would do well"}, {"player": "JAQUES", "playerline": "to set the deer's horns upon his head for a branch of"}, {"player": "JAQUES", "playerline": "victory.--Have you no song, forester, for this purpose?"}, {"player": "SECOND LORD", "playerline": "Yes, sir."}, {"player": "JAQUES", "playerline": "Sing it. 'Tis no matter how it be in tune, so it"}, {"player": "JAQUES", "playerline": "make noise enough."}, {"player": "JAQUES", "playerline": "Music. Song."}, {"player": "SECOND LORD", "playerline": "SECOND LORD [sings]"}, {"player": "SECOND LORD", "playerline": "What shall he have that killed the deer?"}, {"player": "SECOND LORD", "playerline": "His leather skin and horns to wear."}, {"player": "SECOND LORD", "playerline": "Then sing him home."}, {"player": "DIRECTION", "playerline": "[The rest shall bear this burden:]"}, {"player": "SECOND LORD", "playerline": "Take thou no scorn to wear the horn."}, {"player": "SECOND LORD", "playerline": "It was a crest ere thou wast born."}, {"player": "SECOND LORD", "playerline": "Thy father's father wore it,"}, {"player": "SECOND LORD", "playerline": "And thy father bore it."}, {"player": "SECOND LORD", "playerline": "The horn, the horn, the lusty horn"}, {"player": "SECOND LORD", "playerline": "Is not a thing to laugh to scorn."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]