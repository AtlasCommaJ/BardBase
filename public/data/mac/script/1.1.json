[{"player": "ACT", "playerline": "ACT I"}, {"player": "DIRECTION", "playerline": "[Thunder and Lightning. Enter three Witches.]"}, {"player": "FIRST WITCH", "playerline": "When shall we three meet again?"}, {"player": "FIRST WITCH", "playerline": "In thunder, lightning, or in rain?"}, {"player": "SECOND WITCH", "playerline": "When the hurly-burly's done,"}, {"player": "SECOND WITCH", "playerline": "When the battle's lost and won."}, {"player": "THIRD WITCH", "playerline": "That will be ere the set of sun."}, {"player": "FIRST WITCH", "playerline": "Where the place?"}, {"player": "SECOND WITCH", "playerline": "Upon the heath."}, {"player": "THIRD WITCH", "playerline": "There to meet with Macbeth."}, {"player": "FIRST WITCH", "playerline": "I come, Graymalkin."}, {"player": "SECOND WITCH", "playerline": "Paddock calls."}, {"player": "THIRD WITCH", "playerline": "Anon."}, {"player": "ALL", "playerline": "Fair is foul, and foul is fair;"}, {"player": "ALL", "playerline": "Hover through the fog and filthy air."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]