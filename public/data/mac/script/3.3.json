[{"player": "SCENE", "playerline": "SCENE III"}, {"player": "DIRECTION", "playerline": "[Enter three Murderers.]"}, {"player": "FIRST MURDERER", "playerline": "But who did bid thee join with us?"}, {"player": "THIRD MURDERER", "playerline": "Macbeth."}, {"player": "SECOND MURDERER", "playerline": "He needs not our mistrust, since he delivers"}, {"player": "SECOND MURDERER", "playerline": "Our offices and what we have to do"}, {"player": "SECOND MURDERER", "playerline": "To the direction just."}, {"player": "FIRST MURDERER", "playerline": "Then stand with us.--"}, {"player": "FIRST MURDERER", "playerline": "The west yet glimmers with some streaks of day."}, {"player": "FIRST MURDERER", "playerline": "Now spurs the lated traveler apace"}, {"player": "FIRST MURDERER", "playerline": "To gain the timely inn, and near approaches"}, {"player": "FIRST MURDERER", "playerline": "The subject of our watch."}, {"player": "THIRD MURDERER", "playerline": "Hark, I hear horses."}, {"player": "BANQUO", "playerline": "Give us a light there, ho!"}, {"player": "SECOND MURDERER", "playerline": "Then 'tis he. The rest"}, {"player": "SECOND MURDERER", "playerline": "That are within the note of expectation"}, {"player": "SECOND MURDERER", "playerline": "Already are i' th' court."}, {"player": "FIRST MURDERER", "playerline": "His horses go about."}, {"player": "THIRD MURDERER", "playerline": "Almost a mile; but he does usually"}, {"player": "THIRD MURDERER", "playerline": "(So all men do) from hence to th' palace gate"}, {"player": "THIRD MURDERER", "playerline": "Make it their walk."}, {"player": "DIRECTION", "playerline": "[Enter Banquo and Fleance, with a torch.]"}, {"player": "SECOND MURDERER", "playerline": "A light, a light!"}, {"player": "THIRD MURDERER", "playerline": "'Tis he."}, {"player": "FIRST MURDERER", "playerline": "Stand to 't."}, {"player": "BANQUO", "playerline": "It will be rain tonight."}, {"player": "FIRST MURDERER", "playerline": "Let it come down!"}, {"player": "DIRECTION", "playerline": "[The three Murderers attack.]"}, {"player": "BANQUO", "playerline": "O treachery! Fly, good Fleance, fly, fly, fly!"}, {"player": "BANQUO", "playerline": "Thou mayst revenge--O slave!"}, {"player": "DIRECTION", "playerline": "[He dies. Fleance exits.]"}, {"player": "THIRD MURDERER", "playerline": "Who did strike out the light?"}, {"player": "FIRST MURDERER", "playerline": "Was 't not the way?"}, {"player": "THIRD MURDERER", "playerline": "There's but one down. The son is fled."}, {"player": "SECOND MURDERER", "playerline": "We have lost best half of our affair."}, {"player": "FIRST MURDERER", "playerline": "Well, let's away and say how much is done."}, {"player": "DIRECTION", "playerline": "[They exit.]"}]